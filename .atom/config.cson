"*":
  core:
    disabledPackages: [
      "ide-haskell-repl"
      "exception-reporting"
      "metrics"
      "linter-hlint"
    ]
    telemetryConsent: "no"
  editor:
    fontSize: 6
    showInvisibles: true
  "exception-reporting":
    userId: ""
  "git-diff": {}
  "haskell-ghc-mod":
    onMouseHoverShow: ""
  "ide-purescript":
    addNpmPath: true
    autocomplete: {}
    pscIdeServerExe: "/home/jk/.psvm/current/bin/psc-ide-server"
    pursExe: "/home/jk/.psvm/current/bin/purs"
    useCombinedExe: false
  "linter-hlint": {}
  "linter-ui-default":
    panelHeight: 69
    showPanel: true
  minimap:
    plugins:
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "find-and-replace": true
      "find-and-replaceDecorationsZIndex": 0
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "vim-mode-plus":
    startInInsertMode: true
  welcome:
    showOnStartup: false
".psci.purescript.source":
  editor:
    tabLength: 4
".purescript.source":
  editor:
    autoIndentOnPaste: true
    showIndentGuide: true
